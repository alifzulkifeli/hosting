  </div>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  <script src="/webCV/jq.js"></script>
  <script src="">

    $(function () {
      var $foo = $("#foo");
      var $bar = $("#bar");
      function onChange() {
        $bar.val($foo.val());
      }
      $("#foo").change(onChange).keyup(onChange);
    });
  </script>
</body>


</html>