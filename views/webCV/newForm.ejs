      <div>
        <button type="button" onclick=mf("link")>Social</button>
        <div style="display: none;" id="link">
          <div>
            <input value="FB" type="text">
            <input type="text"><br>
            <input value="Twitter" type="text">
            <input type="text"><br>
            <input value="Instagram" type="text">
            <input type="text"><br>
          </div>
        </div>
      </div>

      <div>
        <button type="button" onclick=mf("hobby")>Hobby</button>
        <div style="display: none;" id="hobby">
          <div class="container7">
            <button type="button" class="add_form_field7"> &nbsp;
